# 生产环境配置
MAAS_ENVIRONMENT=production
MAAS_DEBUG=false
MAAS_APP_NAME=MaaS Platform
MAAS_APP_VERSION=1.0.0

# 服务器配置
MAAS_HOST=0.0.0.0
MAAS_PORT=8000
MAAS_RELOAD=false

# 数据库配置（生产数据库 - 需要替换为实际值）
MAAS_DATABASE_URL=postgresql+asyncpg://maas_prod:CHANGE_ME@db-host:5432/maas_prod
MAAS_DATABASE_URL_SYNC=postgresql://maas_prod:CHANGE_ME@db-host:5432/maas_prod

# Redis配置（生产Redis - 需要替换为实际值）
MAAS_REDIS_URL=redis://redis-host:6379/0

# Milvus配置（生产Milvus - 需要替换为实际值）
MAAS_MILVUS_HOST=milvus-host
MAAS_MILVUS_PORT=19530

# JWT配置（生产环境必须使用强密钥）
MAAS_JWT_SECRET_KEY=CHANGE_ME_TO_STRONG_SECRET_KEY_FOR_PRODUCTION
MAAS_JWT_ALGORITHM=HS256
MAAS_JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
MAAS_JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# 安全配置
MAAS_BCRYPT_ROUNDS=12
MAAS_CORS_ORIGINS=["https://your-frontend-domain.com"]
MAAS_FRONTEND_URL=https://your-frontend-domain.com

# 文件存储配置
MAAS_UPLOAD_MAX_SIZE=1073741824
MAAS_UPLOAD_DIR=/app/uploads

# 邮件配置（生产邮件服务 - 需要替换为实际值）
MAAS_SMTP_HOST=smtp.your-email-provider.com
MAAS_SMTP_PORT=587
MAAS_SMTP_USERNAME=your-email@domain.com
MAAS_SMTP_PASSWORD=CHANGE_ME_TO_ACTUAL_PASSWORD
MAAS_SMTP_USE_TLS=true

# 外部服务配置
MAAS_OPENAI_API_KEY=CHANGE_ME_TO_ACTUAL_API_KEY
MAAS_OPENAI_BASE_URL=https://api.openai.com/v1

# 监控配置
MAAS_ENABLE_METRICS=true
MAAS_METRICS_PORT=9090

# 日志配置
MAAS_LOG_LEVEL=INFO
MAAS_LOG_FILE=/app/logs/app.log