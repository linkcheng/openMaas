# 测试环境配置
MAAS_ENVIRONMENT=testing
MAAS_DEBUG=false
MAAS_APP_NAME=MaaS Platform
MAAS_APP_VERSION=1.0.0

# 服务器配置
MAAS_HOST=0.0.0.0
MAAS_PORT=8000
MAAS_RELOAD=false

# 数据库配置（测试数据库）
MAAS_DATABASE_URL=postgresql+asyncpg://maas_test:maas_test@localhost:5432/maas_test
MAAS_DATABASE_URL_SYNC=postgresql://maas_test:maas_test@localhost:5432/maas_test

# Redis配置（测试Redis）
MAAS_REDIS_URL=redis://localhost:6379/1

# Milvus配置
MAAS_MILVUS_HOST=localhost
MAAS_MILVUS_PORT=19530

# JWT配置（测试用较短时间）
MAAS_JWT_SECRET_KEY=test-secret-key-for-testing-only
MAAS_JWT_ALGORITHM=HS256
MAAS_JWT_ACCESS_TOKEN_EXPIRE_MINUTES=5
MAAS_JWT_REFRESH_TOKEN_EXPIRE_DAYS=1

# 安全配置（测试环境使用较低rounds）
MAAS_BCRYPT_ROUNDS=4
MAAS_CORS_ORIGINS=["http://localhost:3000", "http://localhost:8080"]
MAAS_FRONTEND_URL=http://localhost:3000

# 文件存储配置
MAAS_UPLOAD_MAX_SIZE=1073741824
MAAS_UPLOAD_DIR=test_uploads

# 邮件配置（测试环境可能使用mock）
MAAS_SMTP_HOST=localhost
MAAS_SMTP_PORT=1025
MAAS_SMTP_USERNAME=test@example.com
MAAS_SMTP_PASSWORD=test
MAAS_SMTP_USE_TLS=false

# 外部服务配置
MAAS_OPENAI_API_KEY=test-api-key
MAAS_OPENAI_BASE_URL=https://api.openai.com/v1

# 监控配置
MAAS_ENABLE_METRICS=false
MAAS_METRICS_PORT=9090

# 日志配置
MAAS_LOG_LEVEL=DEBUG
MAAS_LOG_FILE=logs/test.log